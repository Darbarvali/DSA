class Solution:
    
    #Function to check if brackets are balanced or not.
     def ispar(self,x):
       # code here
      
       while '()'in x or '{}' in x or '[]' in x:
           x=x.replace('{}','')
           x=x.replace('()','')
           x=x.replace('[]','')
         
           
       if x=='':
           return True
       else:
           return False
